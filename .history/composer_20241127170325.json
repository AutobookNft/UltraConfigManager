{
    "name": "fabiocherici/uconfig",
    "description": "Libreria per la gestione delle configurazioni con caricamento da file e database.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Fabio\\UConfig\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fabio\\UConfig\\Providers\\UConfigServiceProvider"
            ],
            "aliases": {
                "UConfig": "Fabio\\UConfig\\Facades\\UConfig"
            }
        }
    },
    "authors": [
        {
            "name": "Fabio Cherici",
            "email": "fabiocherici@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0"
    },
    "config": {
        "sort-packages": true
    }
} 